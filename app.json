{
  "expo": {
    "name": "React Native Boilerplate",
    "description": "Boilerplate using the following technologies:\n- React native\n- Native base\n- React navigation\n- Gifted chat\n- Redux",
    "slug": "lnolazco-react-native-boilerplate",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "githubUrl": "https://github.com/lnolazco/react-native-boilerplate"
  }
}